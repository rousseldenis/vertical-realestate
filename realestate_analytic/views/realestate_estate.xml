<odoo>
  <record model="ir.ui.view" id="real_estate_form_view">
    <field name="name">real.estate.form (in realestate_estate)</field>
    <field name="model">real.estate</field>
    <field name="inherit_id" ref="realestate_estate.real_estate_form_view" />
    <field name="arch" type="xml">
      <div name="buttons" position="inside">
        <button
          class="oe_stat_button"
          name="button_analytic_entries"
          string="Analytic Items"
          type="object"
          groups="analytic.group_analytic_accounting"
          attrs="{'invisible': [('analytic_account_id', '=', False)]}"
          icon="fa-bars"
        />
      </div>
      <page name="page_contacts" position="after">
        <page
          name="page_analytic"
          string="Analytic"
          groups="analytic.group_analytic_accounting"
        >
          <group name="analytic_left">
            <field name="analytic_account_id" />
          </group>
          <group name="analytic_right">

                    </group>
        </page>
      </page>
    </field>
  </record>
</odoo>
